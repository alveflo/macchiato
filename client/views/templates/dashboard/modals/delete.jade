#delete.ui.basic.modal
  i.close.icon
  .header Delete workspace
  .image.content
    .image
      i.trash.outline.icon
    .description
      p Are you sure you want to delete this workspace? This cannot be undone!
  .actions
    .two.fluid.ui.inverted.buttons
      .ui.red.basic.inverted.button
        p #[i.remove.icon] No
      .ui.green.basic.inverted.button
        p #[i.checkmark.icon] Yes
